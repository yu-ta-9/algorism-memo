{
  "editor.bracketPairColorization.enabled": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "typescript.inlayHints.parameterNames.enabled": "all",
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "editor.codeActionsOnSave": {
    "source.addMissingImports": "explicit",
    "source.fixAll.eslint": "explicit",
    "source.fixAll.stylelint": "explicit"
  },
  "stylelint.validate": ["css", "scss"],
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.formatOnSave": false
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/yarn.lock": true
  },
  "cSpell.words": [],
  "peacock.color": "#ffcc00",
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#ffd633",
    "activityBar.activeBorder": "#abff80",
    "activityBar.background": "#ffd633",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#009f7f",
    "activityBarBadge.foreground": "#e7e7e7",
    "sash.hoverBorder": "#ffd633",
    "statusBar.background": "#ffcc00",
    "statusBar.foreground": "#15202b",
    "statusBarItem.hoverBackground": "#cca300",
    "statusBarItem.remoteBackground": "#ffcc00",
    "statusBarItem.remoteForeground": "#15202b",
    "titleBar.activeBackground": "#ffcc00",
    "titleBar.activeForeground": "#15202b",
    "titleBar.inactiveBackground": "#ffcc0099",
    "titleBar.inactiveForeground": "#15202b99",
    "commandCenter.border": "#15202b99"
  }
}
